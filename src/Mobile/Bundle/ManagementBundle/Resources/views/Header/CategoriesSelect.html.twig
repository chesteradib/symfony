<select 
        data-url="{{path('mobile_category', { 'categoryId' : 0 , 'page' : 0})}}"
        id="shop_bundle_managementbundle_posttype_categories" 
        name="shop_bundle_managementbundle_posttype[categories][]">
        
    <option value="" selected></option>
    {% for alphabet, categories in orderedCategories %}
        {% for category in categories %}
            <option value="{{category['id']}}">
                {{ category['name'] }}
            </option>
        {% endfor %}
    {% endfor %}
</select>


